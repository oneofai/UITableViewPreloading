# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'


inhibit_all_warnings!

target 'UITableViewPreloading' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for UITableViewPreloading

    pod 'AFNetworking'
    pod 'SDWebImage'
    pod 'MJRefresh'

  target 'UITableViewPreloadingTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'UITableViewPreloadingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
